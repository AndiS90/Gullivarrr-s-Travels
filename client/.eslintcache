     />\n             */}\n          {/* )} */}\n        </div>\n      </div>\n    </main>\n  );\n};\n\nexport default Home;\n","/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/pages/Profile.js",["200","201"],"import React from 'react';\n\nimport { Redirect, useParams } from 'react-router-dom';\nimport { useQuery } from '@apollo/client';\n\n\nimport SkillsList from '../components/SkillsList';\nimport SkillForm from '../components/SkillForm';\n\nimport { QUERY_SINGLE_PROFILE, QUERY_ME } from '../utils/queries';\n\nimport Auth from '../utils/auth';\n\nconst Profile = () => {\n  const { profileId } = useParams();\n\n  // If there is no `profileId` in the URL as a parameter, execute the `QUERY_ME` query instead for the logged in user's information\n  const { loading, data } = useQuery(\n    profileId ? QUERY_SINGLE_PROFILE : QUERY_ME,\n    {\n      variables: { profileId: profileId },\n    }\n  );\n\n  // Check if data is returning from the `QUERY_ME` query, then the `QUERY_SINGLE_PROFILE` query\n  const profile = data?.me || data?.profile || {};\n\n  // Use React Router's `<Redirect />` component to redirect to personal profile page if username is yours\n  if (Auth.loggedIn() && Auth.getProfile().data._id === profileId) {\n    return <Redirect to=\"/me\" />;\n  }\n\n  if (loading) {\n    return <div>Loading...</div>;\n  }\n\n  if (!profile?.name) {\n    return (\n      <h4>\n        You need to be logged in to see your profile page. Use the navigation\n        links above to sign up or log in!\n      </h4>\n    );\n  }\n\n  return (\n    <div>\n\n      <h2 className=\"card-header\">\n        This is your Dashboard! You can do whatever you want with it!\n      </h2>\n\n      {/* {profile.skills?.length > 0 && (\n        <SkillsList\n          skills={profile.skills}\n          isLoggedInUser={!profileId && true}\n        />\n      )}\n\n      <div className=\"my-4 p-4\" style={{ border: '1px dotted #1a1a1a' }}>\n        <SkillForm profileId={profile._id} />\n      </div> */}\n    </div>\n  );\n};\n\nexport default Profile;\n","/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/pages/Signup.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/pages/Login.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/Footer/index.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/Header/index.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/utils/mutations.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/utils/auth.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/utils/queries.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/ProfileList/index.js",[],"/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/SkillForm/index.js",["202"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_SKILL } from '../../utils/mutations';\n\nimport Auth from '../../utils/auth';\n\nconst SkillForm = ({ profileId }) => {\n  const [skill, setSkill] = useState('');\n\n  const [addSkill, { error }] = useMutation(ADD_SKILL);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const data = await addSkill({\n        variables: { profileId, skill },\n      });\n\n      setSkill('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h4>Endorse some more skills below.</h4>\n\n      {Auth.loggedIn() ? (\n        <form\n          className=\"flex-row justify-center justify-space-between-md align-center\"\n          onSubmit={handleFormSubmit}\n        >\n          <div className=\"col-12 col-lg-9\">\n            <input\n              placeholder=\"Endorse some skills...\"\n              value={skill}\n              className=\"form-input w-100\"\n              onChange={(event) => setSkill(event.target.value)}\n            />\n          </div>\n\n          <div className=\"col-12 col-lg-3\">\n            <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n              Endorse Skill\n            </button>\n          </div>\n          {error && (\n            <div className=\"col-12 my-3 bg-danger text-white p-3\">\n              {error.message}\n            </div>\n          )}\n        </form>\n      ) : (\n        <p>\n          You need to be logged in to endorse skills. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default SkillForm;\n","/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/SkillsList/index.js",["203"],"import React from 'react';\nimport { useMutation } from '@apollo/client';\n\nimport { REMOVE_SKILL } from '../../utils/mutations';\nimport { QUERY_ME } from '../../utils/queries';\n\nconst SkillsList = ({ skills, isLoggedInUser = false }) => {\n  const [removeSkill, { error }] = useMutation(REMOVE_SKILL, {\n    update(cache, { data: { removeSkill } }) {\n      try {\n        cache.writeQuery({\n          query: QUERY_ME,\n          data: { me: removeSkill },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n    },\n  });\n\n  const handleRemoveSkill = async (skill) => {\n    try {\n      const { data } = await removeSkill({\n        variables: { skill },\n      });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  if (!skills.length) {\n    return <h3>No Skills Yet</h3>;\n  }\n\n  return (\n    <div>\n      <div className=\"flex-row justify-space-between my-4\">\n        {skills &&\n          skills.map((skill) => (\n            <div key={skill} className=\"col-12 col-xl-6\">\n              <div className=\"card mb-3\">\n                <h4 className=\"card-header bg-dark text-light p-2 m-0 display-flex align-center\">\n                  <span>{skill}</span>\n                  {isLoggedInUser && (\n                    <button\n                      className=\"btn btn-sm btn-danger ml-auto\"\n                      onClick={() => handleRemoveSkill(skill)}\n                    >\n                      X\n                    </button>\n                  )}\n                </h4>\n              </div>\n            </div>\n          ))}\n      </div>\n      {error && (\n        <div className=\"my-3 p-3 bg-danger text-white\">{error.message}</div>\n      )}\n    </div>\n  );\n};\n\nexport default SkillsList;\n","/Users/griseldamendieta/Desktop/25-Ins_Resolver-Context/client/src/components/NavBar/index.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/index.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/App.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/reportWebVitals.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/pages/Home.js",["204","205","206"],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/pages/Profile.js",["207","208"],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/pages/Login.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/pages/Signup.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/components/Footer/index.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/components/Header/index.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/utils/mutations.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/utils/auth.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/utils/queries.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/components/ProfileList/index.js",[],"/Users/griseldamendieta/Desktop/Gullivarrrs-Travels/client/src/components/SkillForm/index.js",["209"],"/Users/griseldamendieta/Deskto